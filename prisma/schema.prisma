generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Token {
  id           Int      @id @default(autoincrement())
  access_token String
  refresh_token String?
  scope        String?
  token_type   String?
  expiry_date  BigInt?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
